{
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_title": "Twitter Promote Block"
	},
	"content_scripts": [ {
		"js": ["content.js"],
		"matches": ["https://twitter.com/*"]
	} ],
	"manifest_version": 2,
	"name": "Twitter Promote Block",
	"permissions": ["activeTab"],
	"version": "2019.07.30"
}
